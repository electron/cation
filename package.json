{
  "name": "cation",
  "version": "1.0.0",
  "private": true,
  "description": "Electron's PR monitoring bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --list-different \"{src,spec}/**/*.ts\"",
    "start": "probot run ./lib/index.js",
    "postinstall": "tsc",
    "prettier:write": "prettier --write \"{src,spec}/**/*.ts\"",
    "test": "vitest run --reporter=verbose",
    "coverage": "vitest run --silent --coverage",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/cation.git"
  },
  "author": "Shelley Vohr (@codebytere)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electron/cation/issues"
  },
  "homepage": "https://github.com/electron/cation#readme",
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^3.0.5",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nock": "^14.0.10",
    "prettier": "^3.3.3",
    "smee-client": "^2.0.4",
    "typescript": "^5.8.3",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@sentry/node": "^7.119.2",
    "probot": "^14.2.1",
    "semver": "^7.5.2"
  },
  "lint-staged": {
    "{src,spec}/**/*.ts": "prettier --write"
  },
  "packageManager": "yarn@4.11.0+sha512.4e54aeace9141df2f0177c266b05ec50dc044638157dae128c471ba65994ac802122d7ab35bcd9e81641228b7dcf24867d28e750e0bcae8a05277d600008ad54"
}
